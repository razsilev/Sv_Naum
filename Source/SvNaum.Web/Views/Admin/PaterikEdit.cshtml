@model SvNaum.Web.Models.PaterikModel

@{
    ViewBag.Title = "Edit paterik";
}

<h2>Edit paterik:</h2>

@using (Html.BeginForm("PaterikEdit", "Admin", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    @Html.EditorForModel()

    <br />
    <br />
    <input type="submit" value="Save" />
    <input type="button" value="Cancel" onclick="window.location = '/Home/Index';" />
}

@section scripts
{
    @Scripts.Render("~/bundles/jqueryval");
}
